                const lichTable = document.getElementById('lichTable');
                // Lắng nghe sự kiện người dùng nhập dữ liệu
                searchT1Input.addEventListener("keyup", () => {
                    const searchT1Term = searchT1Input.value.toLowerCase();
                    const searchT2Term = searchT2Input.value.toLowerCase();
                    const rows = lichTable.querySelectorAll('tr');
                    console.log(rows);
