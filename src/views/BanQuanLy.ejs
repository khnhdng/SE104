                        const teamName1 = rows[i].querySelector('td:first-child').textContent.toLowerCase();
                        const teamName2 = rows[i].querySelector('td:nth-child(2)').textContent.toLowerCase();
                        if (teamName1.includes(searchT1Term) && teamName2.includes(searchT2Term)) {
