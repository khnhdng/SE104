            <script>
                var CAUTHU = '<%- dataCauThu %>';
                var DOIBONG = JSON.parse(unescape('<%- parseDataDoiBong %>'));
                const playerTable = document.getElementById('playerTable');
                const THAMSO = JSON.parse(unescape('<%- parseDataThamSo %>'));
                var TuoiCTToiThieu;
                var TuoiCTToiDa;
                var SoCTToiDa;
                var SoCTToiThieu;
                var CTNNToiDa;
                var TuoiCTToiThieu;
                var TuoiCTToiDa;
                for (let i = 0; i < THAMSO.length; i++) {
                    if (THAMSO[i].tenThamSo === 'SoCauThuToiThieu') {
                        SoCTToiThieu = parseInt(THAMSO[i].giaTri);
                    }
                    if (THAMSO[i].tenThamSo === 'SoCauThuToiDa') {
                        SoCTToiDa = parseInt(THAMSO[i].giaTri);
                    }
                    if (THAMSO[i].tenThamSo === 'TuoiCauThuToiDa') {
                        TuoiCTToiDa = parseInt(THAMSO[i].giaTri);
                    }
                    if (THAMSO[i].tenThamSo === 'TuoiCauThuToiThieu') {
                        TuoiCTToiThieu = parseInt(THAMSO[i].giaTri);
                    }
                    if (THAMSO[i].tenThamSo === 'SoCauThuNuocNgoaiToiDa') {
                        CTNNToiDa = parseInt(THAMSO[i].giaTri);
                    }
                }
                console.log(SoCTToiDa);
                //import table from 'handsontable';
                let hot;

                function initializeTable() {
                    var playerColumns = [
                        { title: 'Tên cầu thủ', data: 0 },
                        { title: 'Số áo', data: 1 },
                        { title: 'Vị trí', data: 2, type: 'dropdown', source: ['Thủ môn', 'Hậu vệ', 'Trung vệ', 'Tiền vệ', 'Tiền đạo'] },
                        { title: 'Ngày sinh', data: 3, type: 'date', dateFormat: 'YYYY-MM-DD', defaultValue: 'YYYY-MM-DD', width: '150px' },
                        { title: 'Chiều cao (cm)', data: 4 },
                        { title: 'Cân nặng (kg)', data: 5 },
                        { title: 'Quốc tịch', data: 6 },
                        { title: 'Loại cầu thủ', data: 7, type: 'dropdown', source: ['Cầu thủ nước ngoài', 'Cầu thủ trong nước'], defaultValue: 'Cầu thủ trong nước' },
                        { title: 'Ghi chú', data: 8 }
                    ];

                    hot = new Handsontable(playerTable, {
                        data: [
                            ['', '', '', 'YYYY-MM-DD', '', '', '', 'Cầu thủ trong nước', '']
                        ],
                        columns: playerColumns,
                        rowHeaders: true,
                        stretchH: 'all',
                        columnSorting: true,
                        contextMenu: true,
                        manualRowResize: true,
                        manualColumnResize: true,
                        height: 200,
                        allowRemoveRow: true,
                        selectionMode: 'multiple',
                        someOption: TuoiCTToiDa, TuoiCTToiThieu,
                        scrollbarModel: 'range',
                        scrollbarRange: 5,
                        licenseKey: 'non-commercial-and-evaluation',
                        // afterCreateRow: function (index, amount, source) {
                        //     for (var col = 0; col < playerColumns.length; col++) {
                        //         var defaultValue = playerColumns[col].data;
                        //         hot.setDataAtCell(index, col, defaultValue, 'alter');
                        //     }
                        // },
                        beforeCreateRow: function (index, amount, source) {
                            for (var i = 0; i < playerColumns.length; i++) {
                                var column = playerColumns[i];
                                if (column.defaultValue !== undefined) {
                                    hot.setDataAtCell(index, i, column.defaultValue);
                                }
                            }
                        },
                        afterChange: (changes, source) => {
                            if (source === 'edit') {
                                var quocTichColumn = playerColumns.findIndex(function (column) {
                                    return column.title === 'Quốc tịch';
                                });

                                var loaiCauThuColumn = playerColumns.findIndex(function (column) {
                                    return column.title === 'Loại cầu thủ';
                                });
                                changes.forEach(function (change) {
                                    var row = change[0];
                                    var column = change[1];
                                    var newValue = change[3];
                                    if (column === quocTichColumn) {
                                        if (newValue === 'Việt Nam' || newValue === 'việt nam' || newValue === 'Việt nam' || newValue === 'việt Nam') {
                                            hot.setDataAtCell(row, loaiCauThuColumn, 'Cầu thủ trong nước');
                                        }
                                        else {
                                            hot.setDataAtCell(row, loaiCauThuColumn, 'Cầu thủ nước ngoài');
                                        }
                                    }
                                });
                            }
                        },
                        beforeChange: function (changes, source) {
                            if (source === 'edit') {
                                var ngaySinhColumn = playerColumns.findIndex(function (column) {
                                    return column.title === 'Ngày sinh';
                                });
                                var errorMessages = [];

                                changes.forEach(function (change) {
                                    var row = change[0];
                                    var column = change[1];
                                    var newValue = change[3];

                                    if (column === ngaySinhColumn) {
                                        var ngaySinh = moment(newValue, 'YYYY-MM-DD');
                                        var age = moment().diff(ngaySinh, 'years');

                                        if (age < TuoiCTToiThieu || age > TuoiCTToiDa) {
                                            var errorMess = 'Tuổi cầu thủ phải nằm trong khoảng từ ' + TuoiCTToiThieu + ' đến ' + TuoiCTToiDa;
                                            errorMessages.push(errorMess);
                                        }
                                    }
                                });
                            }
                            if (errorMessages.length > 0) {
                                // Hiển thị thông báo lỗi
                                alert(errorMessages.join('\n'));
                                return false; // Ngăn chặn thực hiện thay đổi
                            }

                            return true; // Cho phép thực hiện thay đổi
                        }
                    });
                }
                // Thêm hàng
                function addPlayerRow() {
                    hot.alter('insert_row_below', hot.countRows());
                }
                // Xóa hàng
                function removePlayerRow() {
                    var currentRowCount = hot.countRows();
                    hot.alter('remove_row', currentRowCount - 1);
                }

                initializeTable();

                function handleSubmit(event) {
                    event.preventDefault();

                    // Lấy thông tin từ các trường nhập
                    const teamName = document.getElementById('id-teamName').value;
                    const coach = document.getElementById('id-coach').value;
                    const homeGround = document.getElementById('id-homeGround').value;
                    const homeJerseyColor = document.getElementById('id-homeJerseyColor').value;
                    const awayJerseyColor = document.getElementById('id-awayJerseyColor').value;
                    const playerData = hot.getData();
                    //Kiểm tra khóa chính 
                    var checkTeamName = false;
                    for (let i = 0; i < DOIBONG.length; i++) {
                        if (teamName === DOIBONG[i].tenDoiBong) {
                            checkTeamName = true;
                        }
                    }

                    var checkCTToiThieuToiDa = true;
                    var checkCTNNToiDa = true;
                    var checkNULLNgaySinh = true;
                    console.log(hot.countRows());
                    let countCT = hot.countRows();
                    let countNN = 0;


                    //Kiểm tra số lượng CTNN
                    for (let i = 0; i < playerData.length; i++) {
                        if (playerData[i][7] === 'Cầu thủ nước ngoài') {
                            countNN++;
                        }
                    }
                    //Kiểm tra số lượng cầu thủ
                    if (countCT < SoCTToiThieu || countCT > SoCTToiDa) {
                        checkCTToiThieuToiDa = false;
                    }

                    if (countNN > CTNNToiDa) {
                        checkCTNNToiDa = false;
                    }
                    //Kiểm tra ngày sinh không null
                    for (let i = 0; i < playerData.length; i++) {
                        if (playerData[i][3] === null || playerData[i][3] === 'YYYY-MM-DD') {
                            checkNULLNgaySinh = false;
                            break;
                        }
                    }
                    console.log(playerData);
                    if (!checkTeamName && checkCTToiThieuToiDa && checkCTNNToiDa && checkNULLNgaySinh) {

                        // Tạo đối tượng đại diện cho đội bóng
                        const team = {
                            teamName,
                            coach,
                            homeGround,
                            homeJerseyColor,
                            awayJerseyColor,
                            players: playerData.slice(0) // Bỏ qua hàng đầu tiên (tiêu đề)
                        };

                        // Cập nhật giá trị của hidden input
                        document.getElementById('playerData').value = JSON.stringify(team.players);

                        // Gửi form
                        document.getElementById('teamForm').submit();
                    }
                    else {
                        if (checkTeamName) {
                            errorTeamName.innerHTML = 'Đội bóng đã tồn tại';
                            errorTeamName.style.color = '#e53535';
                            teamNameBox.classList.add('error-border');
                        }
                        if (!checkCTToiThieuToiDa) {
                            var errorSoCTinner = 'Số lượng cầu thủ phải từ ' + SoCTToiThieu + ' đến ' + SoCTToiDa;
                            errorSoCT.innerHTML = errorSoCTinner;
                            errorSoCT.style.color = '#e53535';
                        }
                        else {
                            errorSoCT.innerHTML = '';
                        }
                        if (!checkCTNNToiDa) {
                            var errorCTNNinner = 'Số lượng cầu thủ nước ngoài tối đa là ' + CTNNToiDa;
                            errorCTNN.innerHTML = errorCTNNinner;
                            errorCTNN.style.color = '#e53535';
                        }
                        else {
                            errorCTNN.innerHTML = '';
                        }
                        if (!checkNULLNgaySinh) {
                            errorNgaySinh.innerHTML = 'Ngày sinh không được để trống';
                            errorNgaySinh.style.color = '#e53535';
                        }
                        else {
                            errorNgaySinh.innerHTML = '';
                        }
                    }
                }
                //Bỏ error
                const errorTeamName = document.getElementById('errorTeamName');
                const errorSoCT = document.getElementById('errorSoCT');
                const errorCTNN = document.getElementById('errorCTNN');
                const errorNgaySinh = document.getElementById('errorNgaySinh');
                const teamNameBox = document.getElementById('id-teamName');

                teamNameBox.addEventListener('keyup', () => {
                    if (event.key !== 'Enter') {
                        errorTeamName.innerHTML = '';
                        teamNameBox.ClassList.remove('error-border');
                    }
                })

                const teamForm = document.getElementById('teamForm');
                teamForm.addEventListener('submit', handleSubmit);
            </script>
        </section>

