                    for (let i = 0; i < rows.length; i++) {
                        var row1 = rows[i];
                        const tenDoi11 = row1.getElementsByTagName('input')[0];
                        const tenDoi12 = row1.getElementsByTagName('input')[1];
                        const ngay1 = row1.getElementsByTagName('input')[4];
                        for (let j = 0; j < rows.length; j++) {
                            if (i != j) {
                                var row2 = rows[j];
                                const tenDoi21 = row2.getElementsByTagName('input')[0];
                                const tenDoi22 = row2.getElementsByTagName('input')[1];
                                const ngay2 = row2.getElementsByTagName('input')[4];
                                if ((tenDoi11.value === tenDoi22.value || tenDoi11.value === tenDoi21.value)
                                    || (tenDoi12.value === tenDoi21.value || tenDoi12.value === tenDoi22.value)) {
                                    if (ngay1.value === ngay2.value) {
                                        checkTrungNgay = false;
                                        // ngay1.classList.add('error-border');
                                        // ngay2.classList.add('error-border');
                                    }
                                    else {
                                        checkTrungNgay = true;
                                        // ngay1.classList.remove('error-border');
                                        // ngay2.classList.remove('error-border');
                                    }
                                }
                            }
                        }
                    }




                    if (checkNgayTheoVong && checkNullNgayThiDau && checkTrungNgay) {
                        document.getElementById('form-laplich').submit();
                    } else {
                        if (!checkNullNgayThiDau) {
                            errorNgayThiDau.innerHTML = 'Ngày giờ không được để NULL.';
                            errorNgayThiDau.style.color = '#e53535';
                        }
                        else {
                            errorNgayThiDau.innerHTML = '';
                        }
                        if (!checkNgayTheoVong) {
                            errorNgayTheoVong.innerHTML = 'Ngày của lượt về phải lớn hơn ngày của lượt đi, và ngược lại.';
                            errorNgayTheoVong.style.color = '#e53535';
                        }
                        else {
                            errorNgayTheoVong.innerHTML = '';
                        }
                        if (!checkTrungNgay) {
                            errorTrungNgay.innerHTML = 'Các ngày thi đấu của một đội nào đó không được trùng nhau';
                            errorTrungNgay.style.color = '#e53535';
                        }
                        else {
                            errorTrungNgay.innerHTML = '';
                        }
                    }
                }

                const formLapLich = document.getElementById('form-laplich');
                formLapLich.addEventListener('submit', handleSubmitLich);
            </script>
